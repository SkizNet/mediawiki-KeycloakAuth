{
	"name": "KeycloakAuth",
	"namemsg": "keycloakauth-extensionname",
	"version": "1.0.0",
	"author": [
		"Ryan Schmidt"
	],
	"license-name": "GPL-3.0-or-later",
	"url": "https://mwusers.org",
	"descriptionmsg": "keycloakauth-desc",
	"type": "other",
	"MessagesDirs": {
		"KeycloakAuth": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"KeycloakAuth\\": "src/"
	},
	"HookHandlers": {
		"main": {
			"class": "KeycloakAuth\\Hooks"
		}
	},
	"Hooks": {
		"GetPreferences": "main",
		"LoadExtensionSchemaUpdates": "main"
	},
	"ServiceWiringFiles": [
		"src/ServiceWiring.php"
	],
	"config_prefix": "wgKeycloakAuth",
	"config": {
		"PortalUrl": {
			"value": "",
			"description": "URL to the account management portal",
			"descriptionmsg": "keycloakauth-config-portalurl"
		}
	},
	"requires": {
		"MediaWiki": ">= 1.35.0",
		"extensions": {
			"Auth_remoteuser": ">= 2.0.0"
		}
	},
	"callback": "KeycloakAuth\\Setup::callback",
	"manifest_version": 2
}
